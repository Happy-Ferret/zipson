{
  "name": "zipson",
  "version": "0.2.0",
  "description": "JSON parse and stringify with compression",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "prepare": "npm run build",
    "w": "run-p w:*",
    "w:compile": "tsc -w 1>/dev/null",
    "w:start": "nodemon -q .",
    "test": "mocha --require ts-node/register --watch-extensions ts 'test/**/*.ts'",
    "testw": "mocha -w --require ts-node/register --watch-extensions ts,jison 'test/**/*.ts'",
    "release": "standard-version"
  },
  "repository": "jgranstrom/zipson",
  "author": "John Granstr√∂m <granstrom.john@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "chai": "^4.1.2",
    "chai-roughly": "^1.0.0",
    "dummy-json": "^2.0.0",
    "mocha": "^5.0.1",
    "mocha-typescript": "^1.1.12",
    "npm-run-all": "^4.1.2",
    "standard-version": "^4.3.0",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.2"
  }
}
